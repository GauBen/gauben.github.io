{
  "name": "gauben.github.io",
  "version": "2.0.0",
  "description": "Personal portfolio",
  "repository": "git@github.com:GauBen/gauben.github.io.git",
  "author": "Gautier Ben AÃ¯m <benaim.gautier@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "yarn build && npm-run-all --parallel 'watch:*'",
    "watch:eleventy": "eleventy --serve",
    "watch:parcel": "PARCEL_WORKERS=0 parcel '.pre-dist/**/*.html' --dist-dir .dist",
    "clean": "rimraf ./.dist ./.pre-dist",
    "build": "yarn clean && yarn build:eleventy && yarn build:parcel",
    "build:eleventy": "eleventy",
    "build:parcel": "PARCEL_WORKERS=0 parcel build '.pre-dist/**/*.html' --dist-dir .dist",
    "test": "yarn test:styl && yarn test:ts && echo ':)'",
    "test:styl": "stylelint './assets/**/*.styl' --custom-syntax stylelint-plugin-stylus/custom-syntax --fix",
    "test:ts": "eslint './assets/**/*.ts' '.eleventy.js' --fix",
    "preview": "yarn build && yarn serve --no-clipboard _dist"
  },
  "devDependencies": {
    "@11ty/eleventy": "11ty/eleventy",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.1.0",
    "@babel/core": "^7.13.16",
    "@parcel/config-default": "2.0.0-nightly.645",
    "@parcel/core": "2.0.0-nightly.643",
    "@parcel/packager-raw-url": "2.0.0-nightly.2267",
    "@parcel/transformer-image": "2.0.0-nightly.2267",
    "@parcel/transformer-stylus": "2.0.0-nightly.645",
    "@parcel/transformer-webmanifest": "2.0.0-nightly.2267",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "eslint": "^7.25.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-html": "^6.1.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "normalize.css": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "parcel": "2.0.0-nightly.643",
    "parcel-reporter-static-files-copy": "^1.3.0",
    "postcss": "^8.2.12",
    "rimraf": "^3.0.2",
    "stylelint": "^13.13.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-plugin-stylus": "^0.10.0",
    "stylelint-rscss": "^0.4.0",
    "typescript": "^4.2.4"
  },
  "volta": {
    "node": "16.0.0",
    "yarn": "1.22.10"
  }
}
